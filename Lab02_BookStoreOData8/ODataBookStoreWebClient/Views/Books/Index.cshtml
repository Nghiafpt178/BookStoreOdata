@model IEnumerable<ODataBookStore.Models.Book>
@{
}
<head>
    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
</head>
<body>

    <h2>HTML Table</h2>

    <table>
        <thead>
            <tr>
                <td>ID</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model){
                <tr>
                    <th>@item.BookId</th>
                </tr>
            }
            
        </tbody>
        
      
    </table>
</body>
